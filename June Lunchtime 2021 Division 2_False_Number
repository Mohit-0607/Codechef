#include <iostream>
using namespace std;

int main() {
    
int t;
    cin >> t;
    while (t--)
    {
        string fal;
        cin >> fal;
        if (fal[0] == '1')
        {
            cout << fal[0];
            cout << "0";
            for (int i = 1; i < fal.size(); i++)
            {
                cout << fal[i];
            }
        }
        else
        {
            cout << "1";
            for (int i = 0; i < fal.size(); i++)
            {
                cout << fal[i];
            }
        }
        cout << "\n";
    }
 // your code goes here
 return 0;
}
